{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "regions": [
        "iad1"
    ],
    "functions": {
        "api/auth.js": {
            "maxDuration": 10,
            "memory": 256
        },
        "api/chat.js": {
            "maxDuration": 60,
            "memory": 1024
        },
        "api/active-timer.js": {
            "maxDuration": 10,
            "memory": 256
        },
        "api/todos/[[...slug]].js": {
            "maxDuration": 10,
            "memory": 256
        },
        "api/sessions/[[...slug]].js": {
            "maxDuration": 10,
            "memory": 256
        },
        "api/srs/[[...slug]].js": {
            "maxDuration": 10,
            "memory": 256
        },
        "api/custom-apps/[[...slug]].js": {
            "maxDuration": 10,
            "memory": 256
        },
        "api/conversations/[[...slug]].js": {
            "maxDuration": 10,
            "memory": 256
        }
    },
    "rewrites": [
        {
            "source": "/api/conversations",
            "destination": "/api/conversations/[[...slug]]"
        },
        {
            "source": "/api/conversations/:path*",
            "destination": "/api/conversations/[[...slug]]"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Strict-Transport-Security",
                    "value": "max-age=31536000; includeSubDomains"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                }
            ]
        },
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*).js",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*).css",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}